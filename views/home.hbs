                  <div
                    class='elements border border-white border-4 fw-bold fs-sm lanthanide'
                  ><span>{{this.symbol}}</span></div>
                {{/if}}
              {{/if}}
            {{/each}}

            {{#each (ValidateRow9 elements.row9)}}
              {{#if (IsEqual this.group 'deleted')}}
