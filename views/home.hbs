                      class='elements border border-white border-4 fw-bold fs-sm alkalineEarth'
                    ><span>{{this.symbol}}</span></div>
                  {{else}}
                    {{#if (IsEqual this.group 'metalloid')}}
                      <div
                        class='elements border border-white border-4 fw-bold fs-sm metalloid'
                      ><span>{{this.symbol}}</span></div>
