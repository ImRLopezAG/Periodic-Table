                          {{else}}
                            {{#if (IsEqual this.group 'transitionMetal')}}
                              <div
                                class='elements border border-white border-4 fw-bold fs-sm transitionMetals'
                              ><span>{{this.symbol}}</span></div>
                            {{else}}
                              <div
                                class='elements border border-white border-4 fw-bold fs-sm otherMetals'
                              ><span>{{this.symbol}}</span></div>
                            {{/if}}
                          {{/if}}
                        {{/if}}
                      {{/if}}
                    {{/if}}
                  {{/if}}
                {{/if}}
              {{/if}}
            {{/each}}
            <!--Row5-->
            {{#each (ValidateRow5 elements.row5)}}
              {{#if (IsEqual this.group 'deleted')}}
                <div
                  class='elements border border-white border-4 fw-bold fs-sm deleted'
